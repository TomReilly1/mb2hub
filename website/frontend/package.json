  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.43",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "happy-dom": "^7.6.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.4"
